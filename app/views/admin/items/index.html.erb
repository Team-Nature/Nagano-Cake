  <h1>商品一覧</h1>
  <table class='table'>
  <thead>
    <tr>
      <th>商品ＩＤ</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= link_to item.name, admin_item_path(item) %></td>
      <td><%= item.price %></td>
      <td><%= item.category.name %></td>
      <td><%= item.status %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
  

